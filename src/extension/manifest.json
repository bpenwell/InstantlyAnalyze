{
  "manifest_version": 3,
  "name": "REI Zillow Tool Scraper",
  "version": "1.1.6",
  "description": "Scrape property data from Zillow based on selected map area.",
  "permissions": [
    "activeTab",
    "scripting",
    "cookies",
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "windows"
  ],
  "host_permissions": [
    "*://www.zillow.com/*",
    "*://localhost/*",
    "*://d1jfoupk9ttnf8.cloudfront.net/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "./v2/popup.html",
    "default_icon": {
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_popup_width": 560,
    "default_popup_height": 500
  },
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "results.html",
        "results.js",
        "contentV2.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["*://www.zillow.com/*"],
      "js": ["contentv2.js"]
    }
  ]
}
