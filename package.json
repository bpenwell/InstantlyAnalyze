{
  "name": "@bpenwell/rei-app-root",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@bpenwell/rei-components": "file:../REI-Components",
    "@bpenwell/rei-layouts": "file:../REI-Layouts",
    "@bpenwell/rei-module": "file:../REI-Module",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.48",
    "aws-lambda": "^1.0.7",
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "npm-run-all": "^4.1.5",
    "react": "^18.0.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "stream-browserify": "^3.0.0",
    "typescript": "^4",
    "web-vitals": "^3.5.1"
  },
  "scripts": {
    "start": "node server.js",
    "lint": "eslint .",
    "devServer": "webpack-dev-server --mode development --open --watch",
    "server": "webpack --mode development --watch",
    "buildProduction": "webpack --mode production",
    "install": "npm-run-all --parallel install:*",
    "install:REI-Layouts": "cd ../REI-Layouts && npm i",
    "install:REI-Components": "cd ../REI-Components && npm i",
    "install:REI-Module": "cd ../REI-Module && npm i",
    "build": "webpack",
    "build:REI-Layouts": "cd ../REI-Layouts && npx tsc",
    "build:REI-Components": "cd ../REI-Components && npx tsc",
    "build:REI-Module": "cd ../REI-Module && npx tsc",
    "subPackageServer": "npm-run-all --parallel watch:*",
    "subPackageServer:REI-Layouts": "cd ../REI-Layouts && npm run build",
    "subPackageServer:REI-Components": "cd ../REI-Components && npm run build",
    "subPackageServer:REI-Module": "cd ../REI-Module && npm run build",
    "test": "react-scripts test",
    "cleanRoot": "del /q package-lock.json && rd /s /q dist && rd /s /q node_modules",
    "clean": "npm-run-all --parallel watch:*",
    "clean:REI-Layouts": "cd ../REI-Layouts && del /q package-lock.json && rd /s /q dist",
    "clean:REI-Components": "cd ../REI-Components && del /q package-lock.json && rd /s /q dist",
    "clean:REI-Module": "cd ../REI-Module && del /q package-lock.json && rd /s /q dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@types/aws-lambda": "^8.10.141",
    "@types/bcryptjs": "^2.4.6",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.0.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
