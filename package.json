{
  "name": "@bpenwell/rei-app-root",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.649.0",
    "@aws-sdk/lib-dynamodb": "^3.649.0",
    "@bpenwell/rei-components": "file:../REI-Components",
    "@bpenwell/rei-layouts": "file:../REI-Layouts",
    "@bpenwell/rei-module": "file:../REI-Module",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@types/react": "^18.2.48",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "archiver": "^7.0.1",
    "aws-lambda": "^1.0.7",
    "axios": "^1.7.3",
    "bcryptjs": "^2.4.3",
    "construct-style-sheets-polyfill": "^3.1.0",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "openai": "^4.54.0",
    "react": "^18.0.0",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "web-vitals": "^3.5.1"
  },
  "scripts": {
    "start": "node server.js",
    "lint": "eslint .",
    "devServer": "webpack-dev-server --mode development --open --watch",
    "server": "webpack --mode development --watch",
    "buildProduction": "webpack --mode production",
    "build": "npx tsc",
    "buildLambda": "npm-run-all --parallel buildLambda:* && npm-run-all --parallel postbuild:*",
    "buildLambda:aiRealEstateAgentAPI": "esbuild src/lambdas/aiRealEstateAgentAPI/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/aiRealEstateAgentAPI/index.mjs --external:aws-sdk",
    "buildLambda:authAPI": "esbuild src/lambdas/authAPI/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/authAPI/index.mjs --external:aws-sdk",
    "buildLambda:propertyData": "esbuild src/lambdas/propertyData/index.js --bundle --minify --platform=node --target=node18 --outfile=dist/lambdas/propertyData/index.js",
    "postbuild:aiRealEstateAgentAPI": "cd dist/lambdas && node zip.js aiRealEstateAgentAPI/",
    "postbuild:authAPI": "cd dist/lambdas && node zip.js authAPI/",
    "postbuild:propertyData": "cd dist/lambdas && node zip.js propertyData/",
    "subPackageServer": "npm-run-all --parallel watch:*",
    "test": "react-scripts test",
    "clean": "rm -rf dist package-lock.json",
    "cleanNode": "rm -rf node_modules"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@types/aws-lambda": "^8.10.142",
    "@types/bcryptjs": "^2.4.6",
    "@types/react-dom": "^18.0.0",
    "@types/react-router-dom": "^5.0.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.9.1",
    "esbuild": "0.23.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-pwa-manifest": "^4.3.0",
    "workbox-webpack-plugin": "^7.1.0"
  },
  "description": "This project was bootstrapped with [Create React App](https://github.com/facebook/create-react-app).",
  "main": "server.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
