  "scripts": {
    "dev": "webpack serve --mode development --open",
    "build": "webpack --mode production",
    "build:libs": "cd ../InstantlyAnalyze-Module && npm run build && cd ../InstantlyAnalyze-Components && npm run build && cd ../InstantlyAnalyze-Layouts && npm run build && cd ../InstantlyAnalyze",
    "test": "echo jest",
    "lint": "eslint .",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "start": "node server.js",
    "server": "webpack --mode development --watch",
    "build:lambda": "npm-run-all --parallel buildLambda:* && npm-run-all --parallel postbuild:*",
    "buildLambda:aiRealEstateAgentAPI": "esbuild src/lambdas/aiRealEstateAgentAPI/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/aiRealEstateAgentAPI/index.mjs --external:aws-sdk --define:self=globalThis --define:window=undefined",
    "buildLambda:userConfigs": "esbuild src/lambdas/userConfigs/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/userConfigs/index.js --external:aws-sdk --define:self=globalThis --define:window=undefined",
    "buildLambda:propertyData": "esbuild src/lambdas/propertyData/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/propertyData/index.js --external:aws-sdk --define:self=globalThis --define:window=undefined",
    "buildLambda:rentEstimate": "esbuild src/lambdas/rentEstimate/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/rentEstimate/index.js --external:aws-sdk --define:self=globalThis --define:window=undefined",
    "buildLambda:rentalReport": "esbuild src/lambdas/rentalReport/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/rentalReport/index.js --external:aws-sdk --define:self=globalThis --define:window=undefined",
    "buildLambda:sendFeedbackEmail": "esbuild src/lambdas/sendFeedbackEmail/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/sendFeedbackEmail/index.js --external:aws-sdk --define:self=globalThis --define:window=undefined",
    "buildLambda:stripeSubscription": "esbuild src/lambdas/stripeSubscription/index.ts --bundle --minify --sourcemap --platform=node --target=node18 --outfile=dist/lambdas/stripeSubscription/index.js --external:aws-sdk --define:self=globalThis --define:window=undefined",
    "postbuild:aiRealEstateAgentAPI": "cd dist/lambdas && node zip.js aiRealEstateAgentAPI/",
    "postbuild:userConfigs": "cd dist/lambdas && node zip.js userConfigs/",
    "postbuild:propertyData": "cd dist/lambdas && node zip.js propertyData/",
    "postbuild:rentEstimate": "cd dist/lambdas && node zip.js rentEstimate/",
    "postbuild:rentalReport": "cd dist/lambdas && node zip.js rentalReport/",
    "postbuild:sendFeedbackEmail": "cd dist/lambdas && node zip.js sendFeedbackEmail/",
    "postbuild:stripeSubscription": "cd dist/lambdas && node zip.js stripeSubscription/",
    "build:theme": "npx cloudscape build --input custom-tokens.json --output src/custom-theme.css",
    "publish:patch": "node scripts/publish-packages.js patch",
    "publish:minor": "node scripts/publish-packages.js minor",
    "publish:major": "node scripts/publish-packages.js major",
    "publish:all": "npm run publish:patch",
    "migrate:npm": "node scripts/migrate-to-npm.js migrate",
    "migrate:rollback": "node scripts/migrate-to-npm.js rollback",
    "import:auto": "node scripts/smart-import.js auto",
    "import:local": "node scripts/smart-import.js local",
    "import:published": "node scripts/smart-import.js published"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
